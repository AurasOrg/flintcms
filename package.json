{
  "name": "flintcms",
  "version": "1.0.2-beta",
  "description": "Content-Focused CMS built on Node.js",
  "main": "index.js",
  "repository": "https://github.com/JasonEtco/flintcms",
  "author": "Jason Etcovitch",
  "license": "MIT",
  "engines": {
    "node": "7.10.0",
    "npm": "^5.0.0"
  },
  "keywords": [
    "cms-framework",
    "nunjucks",
    "templating",
    "mongo",
    "mongodb",
    "cms",
    "documentation",
    "nodejs",
    "react",
    "redux",
    "reactjs",
    "async/await"
  ],
  "scripts": {
    "start": "nodemon --ignore app dev.js",
    "build": "rimraf admin && webpack -p --config ./config/webpack.production.config.js --progress --profile --colors",
    "lint": "eslint **/*.js",
    "prepare": "npm run build"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-devshelf"
    ],
    "rules": {
      "color-named": "always-where-possible",
      "declaration-block-semicolon-newline-after": "always-multi-line",
      "declaration-block-single-line-max-declarations": 3,
      "at-rule-empty-line-before": [
        "always",
        {
          "except": [
            "first-nested"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "autoprefixer": "^7.1.1",
    "axios": "^0.16.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.16.0",
    "camelcase": "^4.0.0",
    "chalk": "^1.1.3",
    "cheerio": "^0.22.0",
    "chunk-manifest-webpack-plugin": "^1.1.0",
    "classnames": "^2.2.5",
    "codemirror": "^5.25.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-alpha.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.2",
    "dotenv": "^4.0.0",
    "draft-js": "^0.10.1",
    "draft-js-export-html": "^0.5.4",
    "es6-promise": "^4.0.5",
    "express": "^4.14.1",
    "express-graphql": "^0.6.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.2",
    "form-serialize": "^0.7.1",
    "graphql": "^0.9.1",
    "history": "^4.6.1",
    "html-to-text": "^3.2.0",
    "html-webpack-plugin": "^2.28.0",
    "img-loader": "^2.0.0",
    "immutability-helper": "^2.1.1",
    "inky": "^1.3.7",
    "jimp": "^0.2.27",
    "juice": "^4.0.2",
    "latest-version": "^3.1.0",
    "moment": "^2.17.1",
    "mongoose": "4.10.0",
    "morgan": "^1.8.0",
    "multer": "^1.3.0",
    "node-sass": "^4.4.0",
    "nodemailer": "^4.0.1",
    "nodemon": "^1.11.0",
    "nunjucks": "^3.0.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "postcss-loader": "^2.0.5",
    "prop-types": "^15.5.10",
    "rand-token": "^0.3.0",
    "react": "^15.5.4",
    "react-codemirror": "^1.0.0",
    "react-color": "^2.11.7",
    "react-dnd": "^2.4.0",
    "react-dnd-html5-backend": "^2.4.1",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "5.0.0-alpha.6",
    "react-transform-hmr": "^1.0.4",
    "react-transition-group": "^1.1.3",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.5",
    "semver-diff": "^2.1.0",
    "socket.io": "^2.0.1",
    "socket.io-client": "^2.0.1",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "^2.8.1",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "cross-env": "^5.0.0",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "stylelint": "^7.7.1",
    "stylelint-config-devshelf": "^1.2.0",
    "stylelint-scss": "^1.4.1"
  }
}
